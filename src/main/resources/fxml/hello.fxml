<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.Button?>

<?import javafx.scene.control.Label?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.tec.anji.fxml.MainController"
            prefHeight="400.0" prefWidth="600.0">
    <Label fx:id="label" text="this is a label"/>
    <Button fx:id="hello" prefWidth="100" prefHeight="100" AnchorPane.topAnchor="100" AnchorPane.leftAnchor="100" text="hello world" onAction="#action"/>
</AnchorPane>